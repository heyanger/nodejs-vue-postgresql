{
  "name": "helloworld",
  "description": "hello world page",
  "version": "0.0.1",
  "author": "helloworld@example.com",
  "scripts": {
    "test": "nyc mocha --timeout=3000",
    "lint": "eslint --ext .js,.vue . client/** server/**",
    "lint:fix": "eslint  --ext .js,.vue --fix . client/** server/**",
    "start": "npm run start:dev",
    "start:dev": "cross-env NODE_ENV=development nodemon server/index.js --ignore client/ --ignore admin/",
    "start:staging": "npm run build:prod && npm run node:staging",
    "start:prod": "npm run build:prod && npm run node:prod",
    "clean:dev": "cross-env NODE_ENV=development node utils/clean.js",
    "clean:staging": "cross-env NODE_ENV=staging node utils/clean.js",
    "clean:prod": "cross-env NODE_ENV=production node utils/clean.js",
    "migrate:dev": "cd server && cross-env NODE_ENV=development ../node_modules/.bin/sequelize db:migrate",
    "migrate:staging": "cd server && cross-env NODE_ENV=staging ../node_modules/.bin/sequelize db:migrate",
    "migrate:prod": "cd server && cross-env NODE_ENV=production ../node_modules/.bin/sequelize db:migrate",
    "node:staging": "pm2 start config/staging.json",
    "node:prod": "pm2 start config/prod.json",
    "build:prod": "npm run webpack:prod",
    "webpack:prod": "cross-env NODE_ENV=production webpack --progress --hide-modules --mode production",
    "webpack:dev": "cross-env NODE_ENV=development webpack --progress --hide-modules --mode development"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^12.0.0-alpha.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.1.0",
    "eslint-plugin-vue": "^4.5.0",
    "eventsource-polyfill": "^0.9.6",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^3.2.0",
    "isomorphic-fetch": "^2.2.1",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.9.0",
    "nodemon": "^1.17.4",
    "path": "^0.12.7",
    "pm2": "^2.10.3",
    "postcss-loader": "^2.1.5",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "vue-loader": "^15.0.9",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.7.0",
    "webpack-cli": "^2.1.3",
    "webpack-merge": "^4.1.2"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "connect-history-api-fallback": "^1.5.0",
    "dayjs": "^1.5.20",
    "es6-promise": "^4.2.4",
    "express": "^4.16.3",
    "fs": "0.0.1-security",
    "helmet": "^3.12.0",
    "helmet-csp": "^2.7.0",
    "lodash": "^4.17.10",
    "pg": "^7.4.2",
    "sequelize": "^4.37.7",
    "vue": "^2.5.16",
    "vue-localstorage": "^0.6.2",
    "vue-router": "^3.0.1",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-hot-middleware": "^2.22.1",
    "winston": "^2.4.2"
  }
}
